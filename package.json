  "prettier": "@haixee/prettier-config",
  "dependencies": {
    "@hamster-bot/eslint-config": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@unocss/eslint-config": "^0.61.5",

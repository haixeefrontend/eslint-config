  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./index.js"
    },
    "./index": {
      "require": "./index.js"
    },
    "./node": {
      "require": "./node.js"
    },
    "./typescript": {
      "require": "./typescript.js"
    },

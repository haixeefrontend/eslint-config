    "eslint-plugin-mocha": "^10.3.0",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.10.0",
    "prettier": "^3.0.0"

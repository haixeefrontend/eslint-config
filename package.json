{
  "name": "@haixee/eslint-config",
  "version": "1.3.0",
  "description": "Haixee frontend common eslint config",
  "license": "CC-BY-SA-4.0",
  "repository": {
    "type": "git+https",
    "url": "git+https://github.com/haixeefrontend/eslint-config.git"
  },
  "files": [
    "index.js",
    "node.js",
    "typescript.js",
    "uniapp.js",
    "vue.js"
